     agent any
     stages {
         stage('build') {
             steps {
                 echo 'Building...'
             }
         }
          stage('deploy to staging') {
             steps {
                 echo 'Deploying...'
             }
         }
          stage('deploy to production') {
             steps {
                 echo 'Deploying...'
             }
         }
     }
 }
